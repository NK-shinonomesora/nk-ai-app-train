        "text": "Java",
        "labels": [
          "programming"
        ]
      },
      {
        "start": 79,
        "end": 83,
        "text": "Java",
        "labels": [
          "programming"
        ]
      }
    ],
    "annotator": 1,
    "annotation_id": 13,
